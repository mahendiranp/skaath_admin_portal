{
  "name": "skaath_admin_portal",
  "homepage": "https://mahediranp.github.io/skaath_admin_portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "bootstrap": "^5.1.3",
    "firebase": "^9.6.7",
    "install": "^0.13.0",
    "npm": "^8.5.2",
    "prop-types": "^15.8.1",
    "ra-data-firebase-client": "^3.0.0",
    "ra-data-json-server": "^3.19.10",
    "react": "^17.0.2",
    "react-admin": "^3.19.10",
    "react-admin-firebase": "^3.2.37",
    "react-dom": "^17.0.2",
    "react-firebaseui": "^6.0.0",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "REACT_APP_FIREBASE_CONFIG=`cat ./FIREBASE_CONFIG.json` NODE_ENV=development BROWSER=none PORT=3333 react-scripts start",
    "build": "REACT_APP_FIREBASE_CONFIG=`cat ./FIREBASE_CONFIG.json` react-scripts build",
    "build-ci": "react-scripts build",
    "cp-dist": "rm -rf ./node_modules/react-admin-firebase/dist && cp -r ../dist ./node_modules/react-admin-firebase/dist",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "predeploy":"npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
}
